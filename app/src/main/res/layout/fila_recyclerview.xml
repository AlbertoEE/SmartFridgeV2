<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="110dp">

    <ImageView
        android:id="@+id/ivFotoAlimentoFila"
        android:layout_width="90dp"
        android:layout_height="88dp"
        android:layout_marginBottom="8dp"
        android:layout_marginStart="16dp"
        android:layout_marginTop="8dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:srcCompat="@drawable/ic_launcher_background" />

    <TextView
        android:id="@+id/tvNombreAlimentoFila"
        android:layout_width="100dp"
        android:layout_height="30dp"
        android:layout_marginStart="16dp"
        android:layout_marginTop="4dp"
        android:text="TextView"
        app:layout_constraintStart_toEndOf="@+id/ivFotoAlimentoFila"
        app:layout_constraintTop_toTopOf="@+id/ivFotoAlimentoFila" />

    <TextView
        android:id="@+id/tvUnidadesFila"
        android:layout_width="100dp"
        android:layout_height="30dp"
        android:layout_marginBottom="4dp"
        android:layout_marginStart="16dp"
        android:text="TextView"
        app:layout_constraintBottom_toBottomOf="@+id/ivFotoAlimentoFila"
        app:layout_constraintStart_toEndOf="@+id/ivFotoAlimentoFila" />

    <TextView
        android:id="@+id/tvFechaCaducidadFila"
        android:layout_width="100dp"
        android:layout_height="30dp"
        android:layout_marginStart="144dp"
        android:layout_marginTop="4dp"
        android:text="TextView"
        app:layout_constraintStart_toEndOf="@+id/ivFotoAlimentoFila"
        app:layout_constraintTop_toTopOf="@+id/ivFotoAlimentoFila" />

    <TextView
        android:id="@+id/tvDiasCaducidadFila"
        android:layout_width="100dp"
        android:layout_height="30dp"
        android:layout_marginBottom="4dp"
        android:layout_marginStart="144dp"
        android:text="TextView"
        app:layout_constraintBottom_toBottomOf="@+id/ivFotoAlimentoFila"
        app:layout_constraintStart_toEndOf="@+id/ivFotoAlimentoFila" />
</android.support.constraint.ConstraintLayout>